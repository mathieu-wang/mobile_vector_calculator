<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sep.vectorcalculator" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TwoVectorsAdditionActivity"
            android:label="@string/title_activity_two_vectors_addition" >
        </activity>
        <activity
            android:name=".ThreeVectorsAdditionActivity"
            android:label="@string/title_activity_three_vectors_addition" >
        </activity>
        <activity
            android:name=".ScalarProductActivity"
            android:label="@string/title_activity_scalar_product" >
        </activity>
        <activity
            android:name=".VectorProductActivity"
            android:label="@string/title_activity_vector_product" >
        </activity>
    </application>

</manifest>
